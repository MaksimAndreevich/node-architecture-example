{
  "name": "example-api-express",
  "version": "1.0.0",
  "description": "example building a node js app on express and TS",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "start": "node ./dist/main.js",
    "dev": "nodemon",
    "dev:inspect": "nodemon -e ts,json --exec node --inspect=localhost:9222 -r ts-node/register src/main.ts",
    "build": "tsc",
    "lint": "eslint ./src/**",
    "lint:fix": "eslint ./src/** --fix",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MaksimAndreevich/example-api-express.git"
  },
  "author": "Maksim Efimov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MaksimAndreevich/example-api-express/issues"
  },
  "homepage": "https://github.com/MaksimAndreevich/example-api-express#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "inversify": "^6.0.1",
    "reflect-metadata": "^0.1.13",
    "tslog": "^3.3.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@typescript-eslint/eslint-plugin": "^5.41.0",
    "@typescript-eslint/parser": "^5.41.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  }
}
